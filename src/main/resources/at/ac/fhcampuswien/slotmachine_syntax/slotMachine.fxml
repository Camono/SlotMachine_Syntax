      <Label fx:id="balanceLabel" layoutX="295.0" layoutY="448.0" prefHeight="27.0" prefWidth="59.0" textOverrun="CLIP">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="betLabel" layoutX="483.0" layoutY="434.0" prefHeight="39.0" prefWidth="59.0" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button id="info" fx:id="infoBtn" layoutX="767.0" layoutY="14.0" mnemonicParsing="false" onAction="#onInfoButtonClick" text="i" />
      <ImageView fx:id="symbol1" fitHeight="70.0" fitWidth="70.0" layoutX="202.0" layoutY="253.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol2" fitHeight="71.0" fitWidth="71.0" layoutX="295.0" layoutY="253.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol3" fitHeight="71.0" fitWidth="71.0" layoutX="380.0" layoutY="253.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol4" fitHeight="71.0" fitWidth="71.0" layoutX="471.0" layoutY="253.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol5" fitHeight="71.0" fitWidth="71.0" layoutX="559.0" layoutY="253.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
