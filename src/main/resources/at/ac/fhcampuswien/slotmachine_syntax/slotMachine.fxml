<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.slotmachine_syntax.Controller.SlotMachineController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0">
         <image>
            <Image url="@/image/Background.png" />
         </image>
      </ImageView>
      <Button fx:id="spinBtn" layoutX="162.0" layoutY="429.0" mnemonicParsing="false" onAction="#onSpinButtonClick" opacity="0.0" prefHeight="47.0" prefWidth="96.0" />
      <Button id="bet+" fx:id="increseBetBtn" layoutX="386.0" layoutY="429.0" mnemonicParsing="false" onAction="#onIncreaseBetButtonClick" opacity="0.0" prefHeight="47.0" prefWidth="65.0" />
