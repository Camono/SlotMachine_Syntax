<AnchorPane maxHeight="675.0" maxWidth="1180.0" minHeight="675.0" minWidth="1180.0" prefHeight="675.0" prefWidth="1180.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.slotmachine_syntax.Controller.SlotMachineController">
   <children>
      <ImageView fitHeight="675.0" fitWidth="1180.0">
         <image>
            <Image url="@../../../../image/Background.png" />
         </image>
      </ImageView>
      <Button fx:id="spinBtn" layoutX="241.0" layoutY="487.0" mnemonicParsing="false" onAction="#onSpinButtonClick" opacity="0.0" prefHeight="44.0" prefWidth="140.0" />
      <Button id="bet+" fx:id="increaseBetBtn" layoutX="573.0" layoutY="486.0" mnemonicParsing="false" onAction="#onIncreaseBetButtonClick" opacity="0.0" prefHeight="50.0" prefWidth="89.0" />
      <Button fx:id="decreaseBetBtn" layoutX="847.0" layoutY="482.0" mnemonicParsing="false" onAction="#onDecreaseBetButtonClick" opacity="0.0" prefHeight="58.0" prefWidth="89.0" />
      <Label fx:id="balanceLabel" alignment="CENTER" layoutX="439.0" layoutY="504.0" prefHeight="25.0" prefWidth="78.0" text="1000.0" textOverrun="CLIP">
         <font>
            <Font name="Baskerville" size="20.0" />
         </font>
      </Label>
      <Label fx:id="betLabel" alignment="CENTER" layoutX="712.0" layoutY="490.0" prefHeight="39.0" prefWidth="86.0" text="1" textAlignment="CENTER">
         <font>
            <Font name="Baskerville Bold" size="24.0" />
         </font>
      </Label>
      <Button id="info" fx:id="infoBtn" layoutX="1132.0" layoutY="23.0" mnemonicParsing="false" onAction="#onInfoButtonClick" text="i" />
      <ImageView fx:id="symbol1ImageView" fitHeight="200.0" fitWidth="95.0" layoutX="306.0" layoutY="226.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol2ImageView" fitHeight="200.0" fitWidth="95.0" layoutX="439.0" layoutY="226.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol3ImageView" fitHeight="200.0" fitWidth="95.0" layoutX="569.0" layoutY="226.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol4ImageView" fitHeight="200.0" fitWidth="95.0" layoutX="698.0" layoutY="226.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="symbol5ImageView" fitHeight="200.0" fitWidth="95.0" layoutX="825.0" layoutY="226.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
